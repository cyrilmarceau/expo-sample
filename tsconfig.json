{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@assets/*": ["./app/core/assets/*"],
            "@components/*": ["./app/core/components/*"],
            "@constants/*": ["./app/core/constants/*"],
            "@hooks": ["./app/core/hooks/redux.ts"],
            "@redux/*": ["./app/core/redux/*"],
            "@navigations/*": ["./app/core/navigations/*"],
            "@routes": ["./app/core/navigations/routes.ts"],
            "@features/*": ["./app/features/*"]
        }
    }
}
